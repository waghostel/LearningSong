{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest",
        "--browserUrl=http://127.0.0.1:9222"
      ],
      "disabled": true,
      "autoApprove": [
        "inspect_element",
        "get_page_state",
        "take_screenshot",
        "list_network_requests",
        "get_network_request",
        "new_page",
        "list_pages"
      ]
    }
  }
}