{
  "enabled": true,
  "name": "Backend Test Report",
  "description": "Runs the full backend test suite using pytest and generates a comprehensive test report in ./report/backend-test/ with errors organized as a todo list from urgent to low risk",
  "version": "1",
  "when": {
    "type": "userTriggered"
  },
  "then": {
    "type": "runCommand",
    "command": "pnpm backend-test-report",
    "cwd": "frontend"
  },
  "workspaceFolderName": "LearningSong",
  "shortName": "backend-test-report"
}